
<% if question.tier == 1 %>
<div class="question-container question-container-radio">
<% else %>
<div class="question-container question-container-radio tier-2">
<% end %>
	<p><%=question.content%></p>
	<label class="radio-inline">
  	<input type="radio" name="questions[<%=question.id%>]" id="inlineRadio1-<%=question.id%>" value="Yes"> Yes
	</label>
	<label class="radio-inline">
  	<input type="radio" name="questions[<%=question.id%>]" id="inlineRadio2-<%=question.id%>" value="No"> No
	</label>
</div>

