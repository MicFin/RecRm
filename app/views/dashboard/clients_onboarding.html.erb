				<h1>QOL Referrals</h1>	
				<table class="table clients-table table-striped datatable">
				  <thead>
				    <tr>
				      <th>First</th>
				      <th>Last</th>
				      <th>Email</th>
				      <th>Invitation Sent</th>
				      <th>Invitation Accepted</th>
				 
				    </tr>
				  </thead>
				  <tfoot>
				    <tr>
				      <th>First</th>
				      <th>Last</th>
				      <th>Email</th>
				      <th>Invitation Sent</th>
				      <th>Invitation Accepted</th>
				 
				    </tr>
				  </tfoot>
				  <tbody>
				    <% @referrals && @referrals.each do |client| %>
				      <tr>
				        <td><%= client.first_name if client.first_name? %></td>
				        <td><%= client.last_name if client.first_name? %></td>
				        <td><%= client.email %></td>
				        <td><%= client.confirmation_sent_at.in_time_zone("Eastern Time (US & Canada)").strftime("%B %d, %Y %I:%M %p") %></td>
				        <td><%= client.confirmed_at.in_time_zone("Eastern Time (US & Canada)").strftime("%B %d, %Y %I:%M %p") if client.confirmed_at?%></td>
