<h1>Deleted</h1>

<table class='table'>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Description</th>
      <th>Deleted by</th>
      <th>At</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @versions.each do |version| %>
      <%- item = version.reify %>
      <tr>
        <td><%= item.id %></td>
        <td><%= item.name %></td>
        <td><%= item.content %></td>
        <td><%#= find_version_author_name(version) %></td>
        <td><%= item.created_at %></td>

        <td><%= link_to 'Bringback', versions_bringback_path(item, version), method: 'PATCH' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%#= link_to 'Back', items_path %>