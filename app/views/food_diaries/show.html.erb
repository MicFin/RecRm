

<div id="family-page" class="container-fluid">
  <div class="row">

    <%= render "/layouts/navbars/user_nav" %>

    <main class="col-xs-10 col-xs-offset-2">
     <section class="alerts-container">
        <%= render "/layouts/alerts" %>
      </section>
      <section>



        <h1><%=@food_diary.user.first_name%>'s Food Diary </h1>
      </section>
      <hr>
      <section class="row">
        <%#= link_to 'New food diary entry', new_food_diary_food_diary_entry_path(@food_diary), class: "btn btn-md btn-default" %>
        <%= render "/food_diary_entries/form", food_diary: @food_diary, food_diary_entry: @food_diary_entry %>
      </section>
      <section class="row">
        <%= render "/food_diary_entries/list", food_diary: @food_diary, food_diary_entry: @food_diary_entry %>


      </section>


    </main>
    <div class="col-xs-10 col-xs-offset-2">
      <%=render "/layouts/footers/main_footer" %>
    </div>
  </div>
</div>




