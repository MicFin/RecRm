
<% if @invoice_price.nil? %> 

	Kindrdfood.welcome.paymentModal.redeemCoupon.showMessage("<%= j render(:partial => '/layouts/alerts') %>");

<% else %>
	
	Kindrdfood.welcome.paymentModal.redeemCoupon.erb("<%=@invoice_cost%>", "<%=@invoice_price%>", "<%=@coupon_description%>", "<%= j render(:partial => '/layouts/alerts') %>");

<% end %>

