

<div id="welcome-page" class="container-fluid">
    <div class="row">

      <%= render "/layouts/navbars/dietitian_nav" %>

      <main class="main-content-container">
        <section class="alerts-container">
          <%= render "/layouts/alerts" %>
        </section>
        <section>
          <div class='row'>
					 	<div class='col-sm-12'>
					 		<%= render "diff_css" %>
					 		<h2><%= "Version #{@version.previous.id} and " + (@version.next ? "Version #{@version.id}" : 'Current Version') %> </h2>
					 		<%= render 'diff_user', version: @version, user: @user %>
					  </div>
					</div>
					<div class="row">
						<div class='col-sm-12'>
					 	  <%= render 'versions/versions_table', versions: @versions %>
					 	</div>	
					</div>
        </section>
      </main>
      <div class="col-xs-12 col-sm-10 col-sm-offset-2">
        <%=render "/layouts/footers/main_footer" %>
      </div>
    </div>
  </div>
