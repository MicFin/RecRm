<div id="welcome-page" class="container-fluid">
		<div class="row">

			<%= render "/layouts/navbars/dietitian_nav" %>

			<main class="col-xs-12 col-sm-10 col-sm-offset-2">

				<!-- ALERTS-->
			  <section class="alerts-container">
	          <%= render "/layouts/alerts" %>
	      </section>
				<section class="jumbotron">
					<div id="jumbotron-inner" class="row">
					 	<div class="form-group col-xs-12 form-group text-center">
					 		<h2>Welcome back to the kitchen, 
								<%= dietitian_name_prompt %>
							</h2>
							<%= dietitian_avatar_prompt %>
					 	</div>
					</div>
				</section>
				<section class="content-container">
					<%= render "welcome/index/upcoming_appointments_table_dietitian", upcoming_appointments: @upcoming_appointments %>
				</section>

				
			</main>
			<div class="col-xs-10 col-xs-offset-2">
				<%=render "/layouts/footers/main_footer" %>
			</div>
		</div>
	</div>


