<div class="table-responsive">
  <table class='datatable responsive no-wrap table table-striped' cellspacing="0" width="100%">
    <thead>
      <tr>
        <th>Version</th>
        <!-- <th>Index</th> -->
        <th>Type</th>
        <th>Event</th>
        <th>Date</th>
        <!--         <th>Author</th> -->
        <th>Editor</th>
        <!-- <th>IP Address</th> -->
        <th></th>
        <th></th>
        <th></th>
        <!-- <th>More Info</th>
        <th>Changeset</th>  -->
      </tr>
    </thead>

    <tbody>
      <% @versions.each do |version| %>
        <tr>
          <td><%= version.id %></td>
          <!-- <td><%#= version.index %></td> -->
          <td><%= version.item_type %></td>
          <td><%= version.event.humanize %></td>
          <td><%= version.created_at %></td>
          <!-- <td><%#= find_version_author_name(version) %></td> -->
          <td><%=version.terminator%></td>
          <!-- <td><%#= version.ip_address %></td> -->
          <%- if version ==  version.item.versions.last %>
            <td>Current Version</td>
            <td></td>
            <td></td>
          <% else %>
            <td><%= link_to 'Diff', versions_diff_path(version.item, version) %></td>
            <td><%= link_to 'Rollback', versions_rollback_path(version.item, version), method: 'PATCH' %></td>
            <td><%#= link_to 'Delete', '' %></td>
          <% end %>
          <!--           <td><%#= version.object %></td>
          <td><%#= version.changeset %></td> -->
        </tr>
      <% end %>
    </tbody>
  </table>
</div>